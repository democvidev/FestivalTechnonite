{% extends 'base.html.twig' %}

{% block title %}Agenda !
{% endblock %}

{% block body %}
	{{ form_start(form) }}
	<div class="row justify-content-center align-items-center">
		<div class="col-md-12 col-xl-4">
			{{ form_row(form.lastName) }}
		</div>
		<div class="col-md-12 col-xl-4">
			{{ form_row(form.firstName) }}
		</div>
		<div class="col-md-12 col-xl-4">
			{{ form_row(form.phone) }}
		</div>
	</div>
	{{ form_end(form) }}
{% endblock %}
